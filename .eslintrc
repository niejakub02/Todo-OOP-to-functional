{
  // "plugins": ["react"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "semi": [2, "always"],
    "indent": ["error", 2, {"SwitchCase": 1}],
    "object-curly-spacing": ["error", "always"],
    "max-len": ["error", { "code": 300 }],
    "no-multi-spaces": "error"
  },
  "settings": {
    "react": {
      "version": "18.0.0"
    }
  }
}